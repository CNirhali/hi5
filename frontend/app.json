{
  "expo": {
    "name": "Commute Connect",
    "slug": "commute-connect",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.commuteconnect.app"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#ffffff"
      },
      "package": "com.commuteconnect.app"
    },
    "plugins": [
      "expo-location",
      "expo-notifications"
    ],
    "sdkVersion": "53.0.0"
  }
} 